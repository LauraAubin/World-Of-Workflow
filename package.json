    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "World of Warcraft",
    "Electron",
    "React",
    "NodeJS",
    "FileSystem"
  ],
  "proxy": "http://localhost:5000/"
